{
  "name": "vscode-twitter",
  "displayName": "Twitter",
  "description": "Twitter in VS Code",
  "icon": "logo.svg",
  "version": "0.1.3",
  "publisher": "austin",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "twitter.start",
        "title": "Twitter: Goto Home Timeline"
      },
      {
        "command": "twitter.post",
        "title": "Twitter: Post Status"
      },
      {
        "command": "twitter.search",
        "title": "Twitter: Search"
      },
      {
        "command": "twitter.select",
        "title": "Twitter: Select Task"
      },
      {
        "command": "twitter.refresh",
        "title": "Twitter: Refresh opened Timelines (those are visible in the editor)"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VSCode Twitter configuration",
      "properties": {
        "twitter.consumerkey": {
          "type": "string",
          "default": "",
          "description": "Specify the consumer key of your twitter app"
        },
        "twitter.consumersecret": {
          "type": "string",
          "default": "",
          "description": "Specify the consumer secret of your twitter app"
        },
        "twitter.accesstokenkey": {
          "type": "string",
          "default": "",
          "description": "Specify the access token of your twitter app"
        },
        "twitter.accesstokensecret": {
          "type": "string",
          "default": "",
          "description": "Specify the access token secret of your twitter app"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "moment": "^2.10.6",
    "twitter": "^1.2.5",
    "xregexp": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/austin-----/vscode-twitter" 
  }
}
